{
  "name": "watapp",
  "version": "1.0.0",
  "description": "View wat files",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "cd ../CLI && pyinstaller --clean -F --add-data=../Analysis/geo_db/:./geo_db/ --add-data=../Analysis/:../Analysis/ ../CLI/wat.py --distpath ../CLI/dist --workpath ../CLI/build --upx-dir ../CLI/build/wat && cd ../watapp && build"
  },
  "build": {
    "appId": "edu.fiu.watapp",
    "mac": {
      "category": "public.app-category.utilities",
      "type": "development"
    },
    "linux": {
      "category": "Development"
    },
    "protocols": [
      {
        "name": "watapp",
        "role": "Viewer",
        "schemes": [
          "watapp"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "../CLI/dist/",
        "to": "CLIBuild"
      },
      {
        "from": "./assets/icon.png",
        "to": "./build/icon.png"
      }
    ],
    "fileAssociations": {
      "ext": "wat",
      "name": "WAT"
    }
  },
  "repository": "",
  "author": "FIU",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.56.0"
  },
  "dependencies": {
    "decompress-zip": "^0.3.0",
    "electron-localshortcut": "^3.1.0",
    "electron-store": "^1.3.0",
    "fs-extra": "^5.0.0",
    "unzip": "^0.1.11"
  }
}
